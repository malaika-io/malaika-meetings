mixin modal
    #exampleModalCenter.modal.fade.add-popup.add-contact-modal(tabindex='-1' role='dialog' aria-hidden='true')
        form(method='POST' action='/users/invite').default-form
            .modal-dialog.modal-dialog-centered(role='document')
                .modal-content
                    .modal-header
                        h2.modal-title
                            | Inviter par Email
                        button.close(type='button' data-dismiss='modal' aria-label='Close')
                            span(aria-hidden='true') &times;
                    .modal-body
                        .form-group
                            h5 Email
                            input#exampleInputEmail1.form-control(type='text', name="email" placeholder='email@test.fr' required autofocus)
                            input(type="hidden" name="_csrf" value=`${csrfToken}`)
                        .modal-footer
                            button.btn.btn-danger.button-effect.btn-sm(type='button' data-dismiss='modal') Annuler
                            button.btn.btn-primary.btn-sm(type='submit' value="Submit") Envoyer


    #audiocall.modal.fade(tabindex='-1' role='dialog'  aria-hidden='true')
        .modal-dialog.modal-dialog-centered(role='document')
            .modal-body
                .audiocall1.call-modal
                    img.bg-img(src='/public/images/avtar/big/audiocall.jpg' alt='Avatar')
                    .center-con.text-center
                        .title2 Josephin water
                        h6 log angelina california
                        ul
                            li
                                a.icon-btn.btn-success.button-effect.btn-xl.is-animating(id="audiorcvcall" data-toggle='modal',data-target='#audiorcvcall')
                                    i(data-feather='phone')
                            li
                                a.icon-btn.btn-danger.button-effect.btn-xl.is-animating.cancelcall(id="audiorcvcall-dismiss",data-dismiss="modal")
                                    i(data-feather='phone')

