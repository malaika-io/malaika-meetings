    #confercall.modal.fade(tabindex='-1' role='dialog'  aria-hidden='true')
        .modal-dialog.modal-dialog-centered(role='document')
            .modal-body
                .conferencecall.call-modal
                    img.bg-img(src='/public/images/avtar/big/audiocall.jpg' alt='Avatar')
                    .center-con.text-center
                        .usersprof
                            .profile
                                img.bg-img(src='/public/images/avtar/big/videocall_bg.jpg' alt='Avatar')
                        h3 Appel  video entrant
                        ul
                            li
                                a.icon-btn.btn-danger.button-effect.btn-xl.is-animating.cancelcall(id="reject-call", data-dismiss="modal")
                                    i(data-feather='phone')
                            li
                                a.icon-btn.btn-success.button-effect.btn-xl.is-animating(id="accept-call")
                                    i(data-feather='video')

    #videocall.viddiolog.modal.fade(tabindex='-1' role='dialog'  aria-hidden='true')
        .modal-dialog.modal-dialog-centered(role='document')
            .modal-body
                .videocall.call-modal
                    .bg-image
                        video(id="videoOutput" autoplay width="640px" height="480px"  poster="/public/images/webrtc.png")
                    .small-image
                        video(id="videoInput" autoplay  width="240px" height="180px" poster="/public/images/webrtc.png")
                    .media.videocall-details
                        .usersprof
                            img.bg-img(src='/public/images/avtar/3.jpg' alt='Avatar')
                        .media-body
                            h5= contact !== null ? `${contact.last_name.toString()}` : ""
                            h6 America ,California
                        #basicUsage 00:00:00
                        .zoomcontent
                            a.text-dark(href='#!' onclick='javascript:toggleFullScreen()',data-tippy-content='Zoom Screen')
                                img(src='/public/images/logo/maximize.svg' alt='zoom screen')
                    .center-con.text-center
                        ul
                            li
                                a.icon-btn.btn-light.button-effect.pause(data-tippy-content='Hold')
                                    i.ti-control-pause
                            li
                                a.icon-btn.btn-danger.button-effect.btn-xl.is-animating(id="stop",data-dismiss="modal",data-tippy-content='Hangup')
                                    i(data-feather='phone')
                            li
                                a.icon-btn.btn-light.button-effect.mic(href="#",data-tippy-content='Mute')
                                    i.fa.fa-microphone

